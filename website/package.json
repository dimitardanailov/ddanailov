{
  "name": "dimitar-danailov-portfolio",
  "version": "1.15.7",
  "main": "index.js",
  "repository": "https://github.com/dimitardanailov",
  "author": "Dimitar Danailov<dimityr.danailov@gmail.com>",
  "license": "MIT",
  "description": "Javascript contractor",
  "scripts": {
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "check-format": "yarn prettier --list-different",
    "format": "yarn prettier --write",
    "clean": "yarn clean:public",
    "clean:public": "rm -rf ./public & mkdir -p public",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint . --fix",
    "publish": "yarn clean && yarn build && yarn gulp && firebase deploy --only hosting:dimitar",
    "reports": "npx jscpd ./"
  },
  "jscpd": {
    "threshold": 0.25,
    "reporters": [
      "html",
      "console"
    ],
    "ignore": [
      ".next"
    ],
    "absolute": false,
    "gitignore": true
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^6.0.0",
    "gulp": "^4.0.2",
    "lerna": "^3.21.0",
    "prettier": "^2.0.2",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@mdx-js/loader": "^1.5.7",
    "@next/mdx": "^9.3.1",
    "@zeit/next-css": "^1.0.1",
    "dotenv": "^8.2.0",
    "next": "^9.2.2",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.0",
    "prismjs": "^1.20.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "5.0.1",
    "typed.js": "^2.0.11"
  }
}
