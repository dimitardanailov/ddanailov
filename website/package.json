{
  "name": "dimitar-danailov-portfolio",
  "version": "1.16.20",
  "main": "index.js",
  "repository": "https://github.com/dimitardanailov",
  "author": "Dimitar Danailov<dimityr.danailov@gmail.com>",
  "license": "MIT",
  "description": "Javascript contractor",
  "scripts": {
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "check-format": "yarn prettier --list-different",
    "format": "yarn prettier --write",
    "clean": "yarn clean:public && yarn clean:next",
    "clean:next": "rm -rf .next/",
    "clean:public": "rm -rf ./public & mkdir -p public",
    "dev": "next",
    "demos:pinterest-layout": "yarn demos:pinterest-layout:dummy && next",
    "demos:pinterest-layout:dummy": "node ./demos/PinterestLayout/dummy/createJSON.js && mv ./records.json demos/PinterestLayout/dummy/",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint . --fix",
    "publish": "yarn clean && yarn build && yarn gulp && firebase deploy --only hosting:dimitar",
    "reports": "npx jscpd ./"
  },
  "jscpd": {
    "threshold": 0.25,
    "reporters": [
      "html",
      "console"
    ],
    "ignore": [
      ".next"
    ],
    "absolute": false,
    "gitignore": true
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "0.0.20",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "gulp": "^4.0.2",
    "lerna": "^3.22.1",
    "prettier": "^2.0.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@mdx-js/loader": "^1.6.16",
    "@next/mdx": "^9.5.1",
    "@xstate/react": "^0.8.1",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.9",
    "axios": "^0.26.0",
    "dotenv": "^10.0.0",
    "graphql": "^15.3.0",
    "immer": "^9.0.6",
    "next": "^9.5.1",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.2",
    "prismjs": "^1.20.0",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-dom": "^17.0.2",
    "react-window": "^1.8.6",
    "styled-components": "5.1.1",
    "typed.js": "^2.0.12",
    "xstate": "^4.11.0"
  }
}
